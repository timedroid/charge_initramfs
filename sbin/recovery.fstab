# mount		fstype	device			[device2]	[fstype2]	[fsoptions]	[fsoptions2]

/boot		mtd	boot
/cache		ext4	/dev/block/mmcblk0p3	NULL		rfs		NULL		llw,check=no,nosuid,nodev
/data		ext4	/dev/block/mmcblk0p1	NULL		rfs		NULL		llw,check=no,nosuid,nodev
/datadata	yaffs2	datadata
/recovery	mtd	recovery
/sdcard		vfat	/dev/block/mmcblk1p1
/system		yaffs2	system
